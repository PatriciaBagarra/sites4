function startTime(){var e=new Date;var t=e.getHours();var r=e.getMinutes();var a=e.getSeconds();t=toTwelveHour(t);r=addLeadingZero(r);document.getElementById("iphone-time").innerHTML=t+":"+r;var s=setTimeout(startTime,1e4)}function addLeadingZero(e){if(e<10){e="0"+e}return e}function toTwelveHour(e){if(e>12){e-=12}else if(e==0){e=12}return e}function empty(){var e;e=document.getElementById("ctr-tool-next").value;if(e==""){return false}}function modifyCallout(){jQuery(".results .overlay").css("display","block");var r=jQuery("#search-change").val();if(r.length>0){jQuery.ajax({type:"POST",url:"/wp-admin/admin-ajax.php",dataType:"json",data:{action:"get_score",keyword:keyword,title_tag:r},success:function(e){jQuery(".callouting p").html(r);var t=e.grade;jQuery(".circle-grade").html(t.charAt(0)+"<span>"+t.charAt(1)+"</span>");jQuery(".ctr-score .circle").attr("stroke-dasharray",e.score+", 100");jQuery("svg.ctr-score").removeClass("red yellow green").addClass(e.color);jQuery(".ctr-characters-grade").html(e.character_count);jQuery(".ctr-characters .circle").attr("stroke-dasharray",e.character_count+", 100");jQuery("svg.ctr-characters").removeClass("red yellow green").addClass(e.title_between_50_60.color);jQuery(".ctr-words-grade").html(e.word_count);jQuery(".primary_keyword_in_title .group div").removeClass("red yellow green").addClass(e.title_contains_keyword.color);jQuery(".primary_keyword_in_title .disp").html(e.title_contains_keyword.description);jQuery(".contains_a_power_word .group div").removeClass("red yellow green").addClass(e.contain_power_words.color);jQuery(".contains_a_power_word .disp").html(e.contain_power_words.description);jQuery(".title_contains_numbers .group div").removeClass("red yellow green").addClass(e.title_contains_numbers.color);jQuery(".title_contains_numbers .disp").html(e.title_contains_numbers.description);jQuery(".contains_special_characters .group div").removeClass("red yellow green").addClass(e.title_contain_special_characters.color);jQue